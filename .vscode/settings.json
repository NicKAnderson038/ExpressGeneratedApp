{
  "vsicons.associations.files": [
    {
      "icon": "config",
      "extensions": ["yml", "yaml"],
      "formate": "svg"
    }
  ],
  "files.autoSave": "onFocusChange",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "eslint.options": {
    "useEslintrc": true,
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "env": {
      "es6": true
    }
  }
}
